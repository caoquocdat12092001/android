<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.manhinhnhaccanhan">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarnhaccanhan"
        android:background="#8F168B"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        tools:ignore="MissingConstraints">

    </androidx.appcompat.widget.Toolbar>

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:drawableLeft="@drawable/ic_baseline_queue_music_24"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="70dp"
                android:paddingLeft="10dp"
                android:fontFamily="@font/bungee_inline"
                android:text="Nhạc cá nhân"
                android:layout_marginBottom="20dp"
                android:textSize="25dp">

            </TextView>
            <ListView
                android:id="@+id/lvnhac1"
                android:layout_marginTop="110dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </ListView>
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/floatingthem1"
                android:layout_gravity="bottom|right"
                android:src="@drawable/add"
                android:layout_marginBottom="20dp"
                android:layout_marginRight="20dp"
                app:fabSize="normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

            </com.google.android.material.floatingactionbutton.FloatingActionButton>
        </androidx.coordinatorlayout.widget.CoordinatorLayout>

        <!--                    <FrameLayout-->
        <!--                        android:id="@+id/framelayout"-->
        <!--                        android:layout_width="match_parent"-->
        <!--                        android:layout_height="match_parent"-->
        <!--                        android:layout_above="@id/menu_bottom">-->

        <!--                    </FrameLayout>-->


    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>